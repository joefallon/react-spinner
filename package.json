    "ts-loader": "^5.3.0",
    "tslib": "^1.9.3",
    "typescript": "^3.1.6",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-command": "^0.4.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-env": "^0.8.0",
    "webpack-merge": "^4.1.4"
