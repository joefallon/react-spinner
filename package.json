    "start": "./node_modules/.bin/webpack-dev-server --config webpack.dev.js --progress",
    "webpack": "./node_modules/webpack-command/lib/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joefallon/react-spinner.git"
  },
  "keywords": [
    "react",
    "react.js",
    "spinner",
    "component",
    "sizable",
    "delayable"
